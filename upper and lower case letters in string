def string_test(s):
    d={"UPPER_COUNT":0, "LOWER_COUNT":0}
    for c in s:
        if c.isupper():
           d["UPPER_COUNT"]+=1
        elif c.islower():
           d["LOWER_COUNT"]+=1
        else:
           pass
    print ("Original String : ", s)
    print ("Total Upper Count : ", d["UPPER_COUNT"])
    print ("Total Lower Count : ", d["LOWER_COUNT"])

string_test('The quick Brow Fox')
